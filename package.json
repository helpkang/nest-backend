{
  "name": "nest-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_OPTIONS='' nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen1": "pnpx openapi-typescript-codegen --client axios -i http://localhost:3000/swagger-yaml -o ./react-client/gen1",
    "gen2": "pnpx @openapitools/openapi-generator-cli generate -g typescript-redux-query -i http://localhost:3000/swagger-yaml -o ./react-client/gen2 --skip-validate-spec",
    "gen3": "pnpx @openapitools/openapi-generator-cli generate -g typescript-fetch -i http://localhost:3000/swagger-yaml -o ./react-client/gen3 --skip-validate-spec",
    "gen4": "pnpx @openapitools/openapi-generator-cli generate -g typescript-axios -i http://localhost:3000/swagger-yaml -o ./react-client/gen4 --skip-validate-spec"
  },
  "dependencies": {
    "@appsignal/nodejs": "^3.0.30",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.7",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.1",
    "form-data": "^4.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "mysql2": "^3.9.0",
    "nest-winston": "^1.9.4",
    "nestjs-cls": "^4.0.3",
    "nestjs-typeorm-transactions": "^2.2.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
